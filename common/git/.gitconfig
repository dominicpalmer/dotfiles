[user]
	name = Dominic Palmer
	email = example@example.com
[core]
	autocrlf = false
	editor = code
[alias]
	st = status
	co = checkout
	cob = checkout -b
	cm = commit -m
	pu = push --set-upstream origin
	wa = worktree add
	wr = worktree remove
	ll = log --oneline
	last = log -1 HEAD --stat
	rv = remote -v
	gl = config --global -l
	re = reset HEAD~
